  "apollo": {
    "schemas": {
      "myPrimaryBackend": {
        "schema": "schema.json",
        "endpoint": "http://localhost:3001/graphql"
      }
    },
    "queries": [
      {
        "schema": "myPrimaryBackend",
        "includes": [
          "src/**/*.ts",
          "src/**/*.graphql"
        ],
        "excludes": [
          "node_modules/**",
          "schema.graphql"
        ]
      }
    ]
